{
  "name": "haute-couture",
  "version": "3.6.0",
  "description": "File-based hapi plugin composer",
  "main": "lib/index.js",
  "engines": {
    "node": ">=8"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "lab -a @hapi/code -t 100 -I \"FinalizationRegistry,WeakRef,AbortController,AbortSignal,EventTarget,Event,MessageChannel,MessagePort,MessageEvent,AggregateError\" && lab -dlL",
    "coveralls": "lab -r lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hapipal/haute-couture.git"
  },
  "keywords": [
    "hapi",
    "scaffold",
    "boilerplate",
    "directory",
    "structure",
    "utility",
    "schwifty",
    "schmervice"
  ],
  "author": "Devin Ivy <devin@bigroomstudios.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hapipal/haute-couture/issues"
  },
  "homepage": "https://github.com/hapipal/haute-couture#readme",
  "dependencies": {
    "@hapi/hoek": "8.x.x",
    "@hapi/topo": "3.x.x",
    "haute": ">=3.1.x <4",
    "parent-module": "2.x.x"
  },
  "peerDependencies": {
    "@hapi/hapi": ">=17 <21"
  },
  "devDependencies": {
    "@hapi/catbox-memory": "4.x.x",
    "@hapi/code": "6.x.x",
    "@hapi/hapi": "18.x.x",
    "@hapi/hapi-20": "npm:@hapi/hapi@20",
    "@hapi/joi": "16.x.x",
    "@hapi/lab": "20.x.x",
    "@hapi/nes": "11.x.x",
    "@hapi/somever": "2.x.x",
    "@hapi/teamwork": "3.x.x",
    "@hapi/vision": "5.x.x",
    "@softonic/hapi-error-logger": "2.x.x",
    "coveralls": "3.x.x",
    "glob": "7.x.x",
    "knex": "0.20.x",
    "objection": "2.x.x",
    "renamer": "1.x.x",
    "schmervice": "1.x.x",
    "schwifty": "4.x.x"
  }
}
